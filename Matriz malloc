#include<stdio.h>
#include<stdlib.h>

void imprimir(int *vet, int n){
    for(int i = 0; i < n; i++){
        printf("%d ", *(vet+i));
    }
}

int main(){
    int n=3, m =4;
    int **mat;
    mat= (int**)malloc(m * sizeof(int*));
    for(int i = 0; i < m; i++){
        mat[i]= (int *)malloc(n*sizeof(int));
    }
    for(int i = 0; i < m ; i++){
        for(int j = 0; j < n ; j++){
            mat[i][j] = rand()%10;
        }
    }
    for(int i = 0; i < m; i++){
        for(int j = 0; j < n; j++){
            printf("%d ", mat[i][j]);
        }
        printf("\n");
    }
    for(int i = 0; i < m; i++){
        free(mat[i]);
    }
    free(mat);
}
